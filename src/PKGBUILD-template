pkgname=micsync
pkgver=0.0.1
pkgrel=1
pkgdesc=""
arch=('any')
url=""
license=('GPL3')
depends=('rsync>=3.1.3' 'python')
makedepends=('python-setuptools')
source=('https://files.pythonhosted.org/packages/source/m/micsync-micdmy/micsync-micdmy-0.0.1.tar.gz')

build() {
	python setup.py build
}

package() {
	python setup.py install --root="$pkgdir/" --optimize=1 --skip-build
}

